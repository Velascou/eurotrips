{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/destinations/delete/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/destinations/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/participants/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/trips/delete/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/trips/([^/]+?)/refresh-flights/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/trips/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/votes/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/weekends/delete/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/weekends/?$",
			"dest": "_render"
		},
		{
			"src": "^/join/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/trips/new/?$",
			"dest": "_render"
		},
		{
			"src": "^/trips/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/trips/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}